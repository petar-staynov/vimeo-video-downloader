{
  "manifest_version": 2,
  "name": "Vimeo Video Downloader",
  "description" : "Download videos from easily from Vimeo.",
  "version": "1.0",
  "icons": { 
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
   "128": "icon128.png" },
  "permissions": [
    "tabs",
    "http://*/",
    "https://*/"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["popup.js"]
  }
}
